     "name": "stdout",
     "output_type": "stream",
     "text": [
      "dcba4321\n"
     ]
    }
   ],
   "source": [
    "def  reverse_string(string):\n",
    "    revst = ''\n",
    "    index = len(string)\n",
    "    while index > 0:\n",
    "        revst += string[ index - 1 ]\n",
    "        index = index - 1\n",
    "    return revst\n",
    "print(reverse_string('1234abcd'))"
   ]
